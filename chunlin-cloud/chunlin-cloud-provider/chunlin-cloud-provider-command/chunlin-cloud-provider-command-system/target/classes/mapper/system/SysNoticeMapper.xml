<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chunlin.cloud.provider.command.system.notice.mapper.SysNoticeMapper">

  <select id="getNoticeList" resultType="com.chunlin.cloud.domain.system.entity.SysNoticeEntity">
    SELECT * FROM sys_notice AS n
    LEFT JOIN sys_user AS u ON n.creator_id = u.user_id
    ${ew.customSqlSegment}
  </select>

</mapper>
